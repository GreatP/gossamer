<li>
  <div  class="thumb">
    <div class="point"></div>
    <div class="protocol">
      <span class="u{{archive.protocol}}">{{archive.protocol}}</span>
    </div>
    <div class="ipadd">
      <div class="srcip"><label>srce. ip</label> <span>{{archive.sourcepre}}</span></div>
      <div class="destip"><label>dest. ip</label> <span>{{archive.destpre}}</span></div>
      <div class="atck-arrow"></div>
    </div>
    <div class="stats">
      <div class="ports">
        <label>affected port{{#if (gt archive.count 1)}}s{{/if}}</label>
        {{#if (lte archive.count 9)}}
        <span class="low">{{archive.count}}</span>
        {{else}}
        {{#if (lte archive.count 30)}}
        <span class="med">{{archive.count}}</span>
        {{else}}
        {{#if (gte archive.count 31)}}
        <span class="high">{{archive.count}}</span>
        {{/if}}
        {{/if}}
        {{/if}}
      </div>
      <div class="pktsize">
        <label>bits (max)</label>
        {{#if (lte archive.maxpktlen 20)}}
        <span class="low">{{archive.maxpktlen}}</span>
        {{else}}
        {{#if (lte archive.maxpktlen 100)}}
        <span class="med">{{archive.maxpktlen}}</span>
        {{else}}
        {{#if (gte archive.maxpktlen 101)}}
        <span class="high">{{archive.maxpktlen}}</span>
        {{/if}}
        {{/if}}
        {{/if}}
      </div>
    </div>
    <div class="time">
      <span>{{moment-to-now (moment-format archive.formatted_frmdate) hidePrefix=true interval=86400}} ago at {{archive.maxfromtime}}</span>
      <span>{{moment-format archive.fromdate 'ddd, MMM Do, YYYY'}}</span>
    </div>
  </div>
</li>