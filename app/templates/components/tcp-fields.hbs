{{!-- {{#each model as |f|}}
  <div class="item" data-value="{{f.value}}">
    {{f.flag}}
  </div>
{{/each}} --}}
<div class="two fields">
  <div class="required twelve wide field">
    <label>Destination IP</label>
    <input type="text" name="tcp-dest[dest-ip]" placeholder="e.g. 4200.1020.9080.8080">
  </div>
  <div class="required four wide field">
    <label>Prefix</label>
    <input type="text" name="tcp-dest[dest-pre]" placeholder="e.g 64">
  </div>
</div>
<div class="required twelve wide field">
  <label>Destination Port</label>
  <input type="text" name="dest-port" placeholder="e.g 8080">
</div>
<div class="required twelve wide field">
  <label>TCP Flags</label>
  {{#ui-dropdown class="fluid search multiple selection" selected=flag onChange=(action (mut flag))}}
    <i class="dropdown icon"></i>
    <div class="default text">Flags</div>
    <div class="menu">
      {{#each mod.tcps as |tcp|}}
        <div class="item" data-value="{{tcp.flag}}">
          {{tcp.flag}}
        </div>
      {{/each}}
    </div>
  {{/ui-dropdown}}

</div>